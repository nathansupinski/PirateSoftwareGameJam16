config_version=5

[application]

config/name="PirateGameJam16"
run/main_scene="res://scenes/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Enums="*res://globals/Enums.gd"
Utils="*res://globals/Utils.gd"
LevelManager="*res://globals/LevelManager.gd"
SignalBus="*res://globals/SignalBus.gd"
ProjectileContainer="*res://globals/ProjectileContainer.tscn"
Global="*res://globals/Global.gd"
StatBroker="*res://globals/StatBroker.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/gaea/plugin.cfg")

[global_group]

Enemy="A bad guy"
player="player character"

[gui]

theme/custom_font="res://asssets/fonts/Audiowide-Regular.ttf"

